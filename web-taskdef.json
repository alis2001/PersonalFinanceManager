{
  "family": "finance-tracker-web",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::819080195356:role/finance-tracker-task-execution-role",
  "taskRoleArn": "arn:aws:iam::819080195356:role/finance-tracker-task-role",
  "containerDefinitions": [
    {
      "name": "web",
      "image": "819080195356.dkr.ecr.eu-south-1.amazonaws.com/finance-tracker/web:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "REACT_APP_API_URL",
          "value": "http://finance-tracker-alb-1958020418.eu-south-1.elb.amazonaws.com/api/auth"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/finance-tracker",
          "awslogs-region": "eu-south-1",
          "awslogs-stream-prefix": "web"
        }
      },
      "memory": 1024,
      "cpu": 512
    }
  ]
}
